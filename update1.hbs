  <h1>Update Info</h1>
  <form action="/student/update" method="post">
    <!-- <strong>Registration No: </strong>{{oldValues.oldregno}} -->
    <div class="form-group">
      <input type="text" class="form-control" name="regno" placeholder="First Name" value="{{oldValues.oldregno}}" readonly>
    </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newFname" placeholder="First Name" value="{{oldValues.oldfname}}">
    </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newLname" placeholder="Last Name" value="{{oldValues.oldlname}}">
    </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newUname" placeholder="Username" value="{{oldValues.olduname}}">
    </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newPhone" placeholder="Phone Number" value="{{oldValues.oldphone}}">
      </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newClass" placeholder="Class" value="{{oldValues.oldclass}}">
      </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newSection" placeholder="Section" value="{{oldValues.oldsec}}">
      </div>
    <div class="form-group">
      <input type="text" class="form-control" name="newMail" placeholder="Email Address" value="{{oldValues.oldmail}}">
      </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
